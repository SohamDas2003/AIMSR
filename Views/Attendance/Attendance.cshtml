@model List<AIMSR.Models.Attendance>

<h2>Attendance Details</h2>

<table border="1">
    <tr>
        <th>Roll No</th>
        <th>Date</th>
        <th>Subject</th>
        <th>Status</th>
    </tr>
    @foreach (var record in Model)
    {
        <tr>
            <td>@record.RollNo</td>
            <td>@record.Date.ToShortDateString()</td>
            <td>@record.Subject</td>
            <td>@record.Status</td>
        </tr>
    }
</table>
